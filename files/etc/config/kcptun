
config general
	option kcptun_client 'nil'
	option client_file '/usr/bin/kcptun/client'
	option enable_server '0'
	option daemon_user 'root'
	option enable_monitor '0'
	option enable_auto_restart '0'
	option enable_logging '0'

config client
	option server '0.0.0.0'
	option server_port '29900'
	option local_host '127.0.0.1'
	option local_port '12948'
	option crypt 'aes'
	option mode 'fast'
	option nocomp 'false'

config server
	option listen_host '127.0.0.1'
	option listen_port '29900'
	option target '0.0.0.0'
	option target_port '12948'
	option crypt 'aes'
	option mode 'fast'
	option nocomp 'false'
